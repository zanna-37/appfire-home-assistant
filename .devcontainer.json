{
  "forwardPorts": [8123],
  "image": "mcr.microsoft.com/devcontainers/python:3",
  "name": "AppFire",
  "portsAttributes": {
    "0-8122": {
      "label": "Auto-Forwarded - Other",
      "onAutoForward": "ignore"
    },
    "8123": {
      "label": "Home Assistant"
    },
    "8124-999999": {
      "label": "Auto-Forwarded - Other",
      "onAutoForward": "ignore"
    }
  },
  "postCreateCommand": "scripts/setup",
  "remoteUser": "vscode",
"runArgs": [
	"-e",
	"GIT_EDITOR=code --wait"
],
"customizations": {
	"vscode": {
		"extensions": [
			"ms-python.black-formatter"
		]
	}
}
}
